{
  "name": "lsicon",
  "version": "0.0.0",
  "description": "Help you to create the icon sets tool",
  "homepage": "https://www.lsicon.com",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wisdesignsystem/lsicon-cli.git"
  },
  "author": "Wis Design <dev@wis.design>",
  "keywords": [
    "cli",
    "icons",
    "react icons"
  ],
  "license": "MIT",
  "type": "module",
  "files": ["lib"],
  "bin": {
    "lsicon": "lib/bin/lsicon.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "changelog": "pnpm changeset",
    "version": "pnpm changeset version",
    "release": "pnpm changeset tag && git push --follow-tags && pnpm publish",
    "clean": "rm -rf node_modules ./lib",
    "build": "rm -rf ./lib && tsc",
    "dev": "tsc -w"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json}": "biome check --write"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.5",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@figma-export/core": "6.0.0",
    "@figma-export/output-components-as-svgr": "6.0.1",
    "@figma-export/transform-svg-with-svgo": "6.0.1",
    "@inquirer/prompts": "7.2.1",
    "@svgr/plugin-jsx": "8.1.0",
    "@tsconfig/node22": "22.0.0",
    "@types/node": "22.10.7",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "ora": "8.0.1",
    "typescript": "5.7.3",
    "zx": "8.2.4"
  }
}
